{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "RAILPACK",
    "nixpacksPlan": {
      "phases": {
        "install": {
          "cmds": ["cd ../.. && bun install"]
        }
      }
    },
    "buildCommand": "cd ../.. && bunx turbo run build --filter=@eiab/demo"
  },
  "deploy": {
    "startCommand": "bun run start"
  }
}
